@model FileUploadViewModel

@{
    ViewData["Title"] = "Home Page";
}

    <form asp-antiforgery="true" method="post" enctype="multipart/form-data" >
        <div class="form-group">
            <label asp-for="FileName">Example file input</label>
            <input asp-for="FileName" class="form-control-file">
        </div>

        <div class="form-group">
            <label asp-for="UploadedFile">Example file input</label>
            <input asp-for="UploadedFile" class="form-control-file">
        </div>
        <button type="submit" class="btn btn-primary mb-2">Submit</button>
    </form>